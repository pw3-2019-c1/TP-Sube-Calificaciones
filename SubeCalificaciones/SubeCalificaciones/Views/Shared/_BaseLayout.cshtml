<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - ¡Sube tus calificaciones!</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Site.css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("AnonimoStyles", required: false)
    @RenderSection("AlumnoStyles", required: false)
    @RenderSection("ProfesorStyles", required: false)
</head>
<body>
    <div class="container">
        @RenderBody()
    </div>
    @Html.Partial("_PartialFooterPage")

    @*Scripts*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
</body>
</html>
