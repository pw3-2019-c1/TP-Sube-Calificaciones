
@{
    ViewBag.Title = "Ingresar";
    Layout = "~/Views/Shared/Anonimo/_AnonimoLayout.cshtml";
}

<form class="form-signin">
    <h2 class="form-signin-heading">Ingresar</h2>
    
    <label for="inputEmail" class="sr-only">Email</label>
    <input type="email" id="inputEmail" class="form-control" placeholder="Ingrese su email" required autofocus>
    
    <label for="inputPassword" class="sr-only">Contraseña</label>
    <input type="password" id="inputPassword" class="form-control" placeholder="Ingrese su contraseña" required>
    
    <div class="checkbox">
        <label>
            <input type="checkbox"> Soy Profesor
        </label>
    </div>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Ingresar</button>
</form>