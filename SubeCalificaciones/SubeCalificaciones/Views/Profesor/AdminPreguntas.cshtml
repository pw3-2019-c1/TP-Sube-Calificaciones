@model IList <SubeCalificaciones.Models.Pregunta >
@{
    ViewBag.Title = "Profesor Home";
    Layout = "~/Views/Shared/Profesor/_ProfesorLayout.cshtml";
}

<h2>Admin Preguntas</h2>

<p>@Html.ActionLink("Crear Pregunta", "CrearPregunta", "Profesor")</p>
<br />
<h4>Preguntas:</h4>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Nro</th>
            <th>Clase</th>
            <th>Tema</th>
            <th>Pregunta</th>
            <th>Fecha Desde</th>
            <th>Fecha Hasta</th>
            <th>Evaluar</th>
            <th>Eliminar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var pregunta in Model)
        {
        <tr>
            <td>@pregunta.Nro</td>
            <td>@pregunta.Clase.Nombre</td>
            <td>@pregunta.Tema.Nombre</td>
            <td>@pregunta.Pregunta1</td>
            <td>@pregunta.FechaDisponibleDesde</td>
            <td>@pregunta.FechaDisponibleHasta</td>
            <td><a>x</a></td>
            <td>x</td>
        </tr>
        }
    </tbody>
</table>